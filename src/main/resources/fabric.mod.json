{
  "schemaVersion": 1,
  "id": "mint",
  "version": "${version}",

  "name": "Mint",
  "description": "Mint and Mint Colored Dye for Minecraft Java Edition!",
  "authors": [
    "Leah (@leafenzio) and Eliza (@tellioaridoitsu)"
  ],
  "contact": {
    "homepage": "ADDME - website link",
    "sources": "https://github.com/Tellios-Projects/Mint"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.leafenzo.mint.ModInit"
    ],
    "client": [
      "net.leafenzo.mint.ModClientInit"
    ],
    "fabric-datagen": [
      "net.leafenzo.mint.datageneration.ModDataGenerator"
    ]
  },
  "mixins": [
    "squashed.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
